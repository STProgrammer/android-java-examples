<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dt.uit.no">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_ic_launcher_round"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name="dt.uit.no.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity 
            android:name="dt.uit.no.VisActivity"
            android:label="@string/title_activity_vis"
            android:theme="@style/AppTheme"
            >
            <intent-filter>
                <action android:name="dt.hin.no.VIS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!-- Funker kun pÃ¥ Android 8.0 (APIlevel <= 26) og eldre versjoner
        <receiver android:name="dt.uit.no.MyReceiver">
            <intent-filter>
                <action android:name="dt.uit.no.VIKTIG_HENDELSE"/>
            </intent-filter>
        </receiver>
        -->


    </application>

</manifest>